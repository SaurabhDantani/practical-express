<div 
    class="modal fade" 
    id="updateMarketMaker<%= marketMaker.MarketMakerId %>" 
    tabindex="-1" 
    role="dialog" 
    aria-hidden="true"
>
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="editDetailModalLabel">Update Market Maker</h5>
        
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body">
        <form action="/marketMaker/update" method="POST">
          <input type="hidden" name="MarketMakerId" value="<%= marketMaker.MarketMakerId %>">

          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <label>Market Maker Name</label>
                <input name="MarketMakerName" required value="<%= marketMaker.MarketMakerName %>" type="text" class="form-control" placeholder="Enter MarketMakerName" />
              </div>
            </div>
          </div>
          <button type="submit" class="btn btn-primary">Submit</button>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">
          Close
        </button>
      </div>
    </div>
  </div>
</div>